
<%= render layout: 'form' do  %>
  <%= form_for @house, url: house_step_path(@house, @step), method: :patch do |f| %>

  <div class="row">
    <div class="col-sm-6 col-sm-offset-3 col-xs-10 col-xs-offset-1">
        <%= render 'houses/photos_fields', f: f %>
    </div>
  </div>
  <div class="row">
    <div class="col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2 col-xs-10 col-xs-offset-1 form-actions">
      <div class="btn-group btn-group-justified" role="group" aria-label="...">
        <div class="btn-group" role="group">
          <%= link_to "Sebelumnya", previous_wizard_path, class: "btn btn-info btn-round-lg" unless @disable_previous %>
        </div>
        <div class="btn-group" role="group">
          
        </div>
      </div>
    </div>
  </div>

  <% end %>
<% end %>