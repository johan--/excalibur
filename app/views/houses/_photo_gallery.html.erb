<div class="row features-container">
  <div class="col-xs-12">
    <% if @photos.empty? %>
      <div class="blankslate">
          <div class="spacious">
              <h3><%= 'Belum Ada Foto' %></h3>
              <p><%= 'Foto-foto untuk rumah ini masih di proses, mohon tunggu' %></p>
          </div>
      </div>
    <% else %>          
      <% @photos.each do |photo| %>
        <%= render partial: 'house_photo', locals: {photo: photo} %>
      <% end %>
    <% end %>
  </div>
</div>

<script type="text/javascript">
  document.getElementById("BT1").addEventListener("click", function(){
    $("body").addClass("disable-nprogress");
  });
</script>