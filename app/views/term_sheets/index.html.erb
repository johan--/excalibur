<p id="notice"><%= notice %></p>

<h1>Listing Term Sheets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @term_sheets.each do |term_sheet| %>
      <tr>
        <td><%= link_to 'Show', term_sheet %></td>
        <td><%= link_to 'Edit', edit_term_sheet_path(term_sheet) %></td>
        <td><%= link_to 'Destroy', term_sheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Term sheet', new_term_sheet_path %>
