<li class="ticket-item">
  <div class="row">
      <div class="ticket-user col-md-4 col-sm-12 col-xs-12">
        <span class="user-name">Proposal <%= tender.barcode %></span>
        <span class="user-company"><%= tender.aqad %></span>
      </div>
      <div class="ticket-time  col-md-4 col-sm-6 col-xs-12">
          <div class="divider hidden-md hidden-sm hidden-xs"></div>
          <i class="fa fa-clock-o"></i>
          <span class="time">@ <%= idr_money(tender.target) %></span>
      </div>
      <div class="ticket-user col-md-2 col-sm-6 col-xs-6">
        <span class="user-at"><%= link_to "lihat", tender_path(tender) %></span>        
      </div>
      <div class="ticket-type  col-md-2 col-sm-6 col-xs-6">
          <span class="divider hidden-xs"></span>
          <span class="type"><%= tender.state %></span>
      </div>
      <div class="ticket-state bg-palegreen">
          <i class="fa fa-check"></i>
      </div>
  </div>
</li>